//       2018:  0.130  0.029
//       2019:  0.130  0.030
//       2020:  0.140  0.030
//       2021:  0.140  0.032
//       2022:  0.140  0.032
{
    "FICA_ss_trt_employer": {
        "2018": 0.065,
        "2020": 0.070
    },
    "FICA_ss_trt_employee": {
        "2018": 0.065,
        "2020": 0.070
    },
    "FICA_mc_trt_employer": {
        "2019": 0.015,
        "2021": 0.016
    },
    "FICA_mc_trt_employee": {
        "2019": 0.015,
        "2021": 0.016
    }         
}
